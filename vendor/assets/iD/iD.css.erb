/* Preset form address */

.form-field .addr-housename {
    border: 0;
}

.form-field .addr-number {
    width: 33%;
    border-left: 0;
    border-right: 0;
    border-bottom: 0;
    border-radius: 0;
}

.form-field .addr-street {
    width: 66%;
    border-right: 0;
    border-bottom: 0;
    border-radius: 0;
}

.form-field .addr-city {
    width: 66%;
    border-left: 0;
    border-right: 0;
    border-bottom: 0;
    border-radius: 0 0 0 4px;
}

.form-field .addr-postcode {
    width: 33%;
    border-right: 0;
    border-bottom: 0;
    border-radius: 0 0 4px 0;
}
/* combobox dropdown */

div.combobox {
    z-index: 9999;
    display: none;
    box-shadow: 0 0 10px 0 rgba(0,0,0,.1);
    margin-top: -1px;
    background: white;
    max-height: 120px;
    overflow-y: auto;
    overflow-x: hidden;
    border: 1px solid #ccc;
    border-radius: 0 0 4px 4px;
}

.combobox a {
    display: block;
    padding: 5px 10px;
    border-top:1px solid #ccc;
