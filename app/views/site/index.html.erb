<% content_for :head do %>
  <%= javascript_include_tag "index" %>
  <%= stylesheet_link_tag 'browse' %>
<% end %>

<%= render :partial => 'home_link' %>
<%= render :partial => 'sidebar' %>
<%= render :partial => 'search' %>

<noscript>
  <div id="noscript">
    <p><%= t 'site.index.js_1' %></p>
    <p><%= t 'site.index.js_2' %></p>
  </div>
</noscript>

  <div id="owl-beta-info">
    Welcome to New History Tab beta! Feel free to
    <a href="https://github.com/ppawel/openstreetmap-website/issues/new" target="_blank">report a problem or suggestion</a>.
    <br>
    Current status: <span style="color: green">OWL API OK - HISTORY PROCESSING IN PROGRESS</span>
  </div>

<div id="map-ui">
</div>

<div id="map" tabindex="2">
</div>

<div id="attribution">
  <table width="100%">
    <tr>
      <td class="attribution_license"><%= t'site.index.license.license_url' %></td>
      <td class="attribution_project"><%= t'site.index.license.project_url' %></td>
    </tr>
    <tr>
      <td colspan="2" class="attribution_notice"><%= t'site.index.license.copyright' %></td>
    </tr>
  </table>
</div>
 
