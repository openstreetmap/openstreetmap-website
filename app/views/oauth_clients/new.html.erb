<% content_for :heading do %>
  <h1><%= t'oauth_clients.new.title' %></h1>
<% end %>

<div class='standard-form'>
  <%= form_for :client_application, :url => { :action => :create } do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
    <%= submit_tag t('oauth_clients.new.submit') %>
  <% end %>
</div>
