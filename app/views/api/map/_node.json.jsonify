json.type "node"
json.id node.id
if node.visible
  json.lat node.lat.to_f
  json.lon node.lon.to_f
end
json.timestamp node.timestamp.xmlschema
json.version node.version
json.changeset node.changeset_id
json.user node.changeset.user.display_name
json.uid node.changeset.user_id
if !node.visible
  json.visible node.visible
end

if !node.tags.empty?
  json.tags node.tags
end
