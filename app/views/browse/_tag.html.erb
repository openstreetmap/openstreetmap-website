<tr>
  <th class='browse-tag-k'><%= format_key(tag[0]) %></th>
  <td class='browse-tag-v'><%= format_value(tag[0], tag[1][0]) %></td>
  <% if tag[1][1] %>
      <td class='browse-tag-v'>
          Last updated by <%= changeset_user_link(tag[1][1][:changeset]) %>
          in version #<%= tag[1][1][:version] %>
      </td>
  <% end %>
</tr>
