<!-- ========= Accommodation ========= -->
  <feature name="Hotel">
    <category>accommodation</category>
    <icon image="features/pois/accommodation_hotel.n.24.png"/>
    <help>http://wiki.openstreetmap.org/wiki/Tag:tourism%3Dhotel</help>
    <point/>
    <area/>
    <tag k="tourism" v="hotel"/>
    <inputSet ref="simpleName"/>
    <inputSet ref="common"/>
  </feature>


